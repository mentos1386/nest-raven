{
  "name": "nest-raven",
  "version": "9.0.0",
  "description": "Sentry Raven Module for Nest Framework",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "format": "prettier \"{example,lib,test}/**/*.ts\" --write",
    "lint": "prettier \"{example,lib,test}/**/*.ts\"  --check",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "start": "ts-node -r tsconfig-paths/register example/src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mentos1386/nest-raven.git"
  },
  "keywords": [
    "nestjs",
    "nest",
    "raven",
    "sentry",
    "module"
  ],
  "author": "Tine Jozelj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mentos1386/nest-raven/issues"
  },
  "homepage": "https://github.com/mentos1386/nest-raven#readme",
  "optionalDependencies": {
    "@nestjs/apollo": "^10.0.0",
    "@nestjs/graphql": "^10.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0",
    "@sentry/node": "^7.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^7.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@nestjs/common": "9.1.6",
    "@nestjs/core": "9.1.6",
    "@nestjs/platform-express": "9.1.6",
    "@nestjs/platform-socket.io": "9.1.6",
    "@nestjs/testing": "9.1.6",
    "@nestjs/websockets": "9.1.6",
    "@sentry/node": "7.17.3",
    "@sentry/types": "7.17.3",
    "@types/jest": "27.5.2",
    "@types/node": "18.11.9",
    "@types/supertest": "2.0.12",
    "@types/ws": "8.5.3",
    "apollo-server-core": "3.10.4",
    "apollo-server-express": "3.10.4",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "dotenv": "16.0.3",
    "graphql": "16.6.0",
    "husky": "8.0.1",
    "jest": "27.5.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.7",
    "socket.io-client": "4.5.3",
    "supertest": "6.3.1",
    "ts-jest": "27.1.5",
    "ts-morph": "16.0.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.8.4"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "collectCoverageFrom": [
      "lib/**"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  }
}
