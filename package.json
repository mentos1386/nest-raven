{
  "name": "nest-raven",
  "version": "5.0.0",
  "description": "Sentry Raven Module for Nest Framework",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && cp *.md dist && cp LICENSE dist && cp package.json dist",
    "format": "prettier \"{example,lib,test}/**/*.ts\" --write",
    "lint": "prettier \"{example,lib,test}/**/*.ts\"  --check",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "test": "jest",
    "start": "ts-node -r tsconfig-paths/register example/src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mentos1386/nest-raven.git"
  },
  "keywords": [
    "nestjs",
    "nest",
    "raven",
    "sentry",
    "module"
  ],
  "author": "Tine Jozelj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mentos1386/nest-raven/issues"
  },
  "homepage": "https://github.com/mentos1386/nest-raven#readme",
  "dependencies": {
    "@sentry/minimal": "^5.12.0",
    "@sentry/node": "^5.12.3"
  },
  "peerDependencies": {
    "@nestjs/graphql": "^6.2.4"
  },
  "devDependencies": {
    "@nestjs/common": "^7.0.2",
    "@nestjs/core": "^7.0.2",
    "@nestjs/graphql": "^7.0.6",
    "@nestjs/platform-express": "^7.0.2",
    "@nestjs/platform-socket.io": "^7.0.2",
    "@nestjs/testing": "^7.0.2",
    "@nestjs/websockets": "^7.0.2",
    "@types/jest": "^25.1.2",
    "@types/node": "^12.0.0",
    "@types/socket.io-client": "^1.4.32",
    "apollo-server-express": "^2.9.16",
    "apollo-server-testing": "^2.9.16",
    "coveralls": "^3.0.3",
    "dotenv": "^8.2.0",
    "graphql": "^14.6.0",
    "jest": "^25.1.0",
    "prettier": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "socket.io-client": "^2.2.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.9.0",
    "type-graphql": "^0.17.6",
    "typescript": "^3.7.5"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "collectCoverageFrom": [
      "lib/**"
    ]
  }
}
